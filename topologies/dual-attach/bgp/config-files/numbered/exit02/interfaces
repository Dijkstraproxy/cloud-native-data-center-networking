auto all

auto lo
iface lo inet loopback

auto mgmt
iface mgmt
  address 127.0.0.1/8
  vrf-table auto

auto eth0
iface eth0 inet dhcp
  vrf mgmt


iface internet-vrf
	address 10.0.0.102/32
	vrf-table auto

# uplinks
iface swp1

iface swp2

# uplinks
iface swp3

iface swp4

iface swp5

iface swp5.2

iface swp5.4
	vrf internet-vrf

iface swp6
       vrf internet-vrf
